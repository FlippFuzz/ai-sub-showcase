        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "04:13.800",
        "end": "04:16.000",
        "original": "8歳なの！",
        "english": "I'm 8 years old!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "04:17.000",
        "end": "04:18.500",
        "original": "8歳なんだー",
