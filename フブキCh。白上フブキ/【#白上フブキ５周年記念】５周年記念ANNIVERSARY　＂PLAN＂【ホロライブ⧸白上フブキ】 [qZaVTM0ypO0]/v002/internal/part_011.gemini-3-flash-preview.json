        "type": "on_screen_text"
      },
      {
        "start": "00:45.300",
        "end": "00:46.400",
        "original": "アキさん。",
        "english": "Aki-san.",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "00:46.500",
        "end": "00:49.000",
        "original": "あ、アキ・ローゼンタールちゃんですね。",
        "english": "Ah, it's Aki Rosenthal-chan.",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
