        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "03:26.700",
        "end": "03:28.100",
        "original": "おかしいだろ！",
        "english": "This is messed up!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "03:28.400",
        "end": "03:30.800",
        "original": "いや早い、早い、早いよ！早いだろお前！",
        "english": "Whoa, wait, too fast! You're too quick!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "03:31.000",
        "end": "03:32.400",
        "original": "ヤバいでしょ！",
        "english": "This is crazy!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "03:33.400",
        "end": "03:34.500",
        "original": "え、ちょっと… どうぞどうぞ？",
