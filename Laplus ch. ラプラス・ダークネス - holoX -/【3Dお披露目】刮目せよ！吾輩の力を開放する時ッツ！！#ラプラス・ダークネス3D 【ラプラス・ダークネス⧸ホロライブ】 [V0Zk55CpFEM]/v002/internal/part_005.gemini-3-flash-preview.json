        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "04:13.100",
        "end": "04:14.500",
        "original": "いただきます",
        "english": "Let's eat.",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
