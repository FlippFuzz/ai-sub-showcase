      },
      {
        "start": "04:53.600",
        "end": "04:57.500",
        "original": "恋はいつも　よそよそしい",
        "english": "Love is always distant.",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "04:57.600",
