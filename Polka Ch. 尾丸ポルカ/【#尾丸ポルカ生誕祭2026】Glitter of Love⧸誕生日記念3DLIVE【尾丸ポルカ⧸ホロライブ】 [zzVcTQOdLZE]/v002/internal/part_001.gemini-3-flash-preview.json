      },
      {
        "start": "01:47.300",
        "end": "01:50.500",
        "original": "君は光の戦士",
        "english": "You are a warrior of light",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "01:52.900",
        "end": "01:56.200",
        "original": "あの日胸に灯った",
