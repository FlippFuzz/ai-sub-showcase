      {
        "start": "02:00.000",
        "end": "02:02.100",
        "original": "バウバウ！",
        "english": "Bau bau!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "02:02.100",
        "end": "02:05.100",
        "original": "はぁ～、バウバウ様～、はぁ～、パトラ様～",
