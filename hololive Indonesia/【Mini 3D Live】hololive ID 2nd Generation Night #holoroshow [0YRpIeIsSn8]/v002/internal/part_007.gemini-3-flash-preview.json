        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "04:52.200",
        "end": "04:54.000",
        "original": "ああ... 大人の事情ね。",
        "english": "Oh... Adult circumstances.",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
