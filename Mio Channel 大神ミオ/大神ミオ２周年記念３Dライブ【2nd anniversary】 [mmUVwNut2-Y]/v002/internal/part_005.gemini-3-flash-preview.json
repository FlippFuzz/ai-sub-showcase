    "subtitles": [
      {
        "start": "00:00.000",
        "end": "00:01.383",
        "original": "本当にいつもありがとうね。",
        "english": "Thank you truly, as always.",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "00:01.383",
        "end": "00:02.503",
        "original": "いやいや、ありがとうミオみーん。",
