      },
      {
        "start": "00:21.200",
        "end": "00:23.800",
        "original": "はい、みなさーん！",
        "english": "Hi everyone!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "00:23.800",
        "end": "00:26.500",
        "original": "こんねぽらぼー！",
