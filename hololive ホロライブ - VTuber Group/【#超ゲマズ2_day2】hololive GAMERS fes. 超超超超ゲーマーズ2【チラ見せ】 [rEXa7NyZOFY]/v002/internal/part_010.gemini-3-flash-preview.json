        "type": "dialogue"
      },
      {
        "start": "03:26.500",
        "end": "03:28.000",
        "original": "おらー！",
        "english": "Hah!",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
      },
      {
        "start": "03:28.100",
        "end": "03:30.200",
        "original": "赤いの来たぞ、見ろ！",
        "english": "The red one's coming, look!",
