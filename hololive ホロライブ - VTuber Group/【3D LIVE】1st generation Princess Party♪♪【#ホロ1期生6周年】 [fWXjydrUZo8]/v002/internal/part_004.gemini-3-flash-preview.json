      },
      {
        "start": "03:03.000",
        "end": "03:07.500",
        "original": "もしかして君と釣り合ってないかな？",
        "english": "Maybe I'm not a good match for you?",
        "alignment_source": "audio_tokens",
        "type": "dialogue"
